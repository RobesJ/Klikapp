{
  "expo": {
    "name": "klikon-crm",
    "slug": "klikon-crm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg",
    "scheme": "klikoncrm",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robesj.klikoncrm",
      "associatedDomains": ["applinks.klikoncrm.applinks"]
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg",
        "backgroundImage": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg",
        "monochromeImage": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "klikoncrm"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.robesj.klikoncrm"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Aplikácia potrebuje prístup k fotoaparátu na fotenie."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
      //"reactCompiler": true
    }
  }
}
