{
  "expo": {
    "name": "Klikapp",
    "slug": "Klikapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg",
    "main": "expo-router/entry",
    "scheme": "klikapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.robesj.Klikapp",
      "associatedDomains": [
        "applinks.Klikapp.applinks"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg",
        "backgroundImage": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg",
        "monochromeImage": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "Klikapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.robesj.KlikApp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/chimney-sweep-holding-sweeper-logo-vector.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Aplikácia potrebuje prístup k fotoaparátu na fotenie."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6f9f3a84-4075-4779-bb8a-ffbfc74d9c70"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6f9f3a84-4075-4779-bb8a-ffbfc74d9c70"
    }
  }
}
